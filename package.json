{
  "name": "interactive-card-details-form-main",
  "private": true,
  "version": "0.0.0",
  "author": "slightlyfunctional",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/interactive-card-details-form/",
    "preview": "vite preview",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --no-cache"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/svelte": "^4.0.1",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "modern-css-reset": "^1.4.0",
    "svelte": "^3.55.1",
    "svelte-jester": "^2.3.2",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "@fontsource/space-grotesk": "^4.5.13"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "^.+\\.svelte": "svelte-jester",
      ".+\\.(css|styl|less|sass|scss|png|svg|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/src/setupTests.js"],
    "moduleNameMapper": {
      "^.+.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  }
}
